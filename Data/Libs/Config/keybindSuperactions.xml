		<action name="sword_rotation_up" map="sharpening" />
		<action name="dice_up" map="dice" />
		<action name="horse_accelerate" map="horse" />
		
		<control input="w" controller="keyboard" />
		<control input="up" controller="keyboard" />
	</superaction>

	<superaction name="down" ui_group="general" ui_name="@ui_keybinds_down" ui_tooltip="" keyboard="writeable">
