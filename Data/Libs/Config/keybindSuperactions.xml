<!--============== conflicting groups =========================================-->
	<!-- basic groups -->
	<conflict name="directions">
		<superaction name="left" />
		<superaction name="right" />
		<superaction name="up" />
		<superaction name="down" />
	</conflict>

	<conflict name="actions">
		<superaction name="primary_action" />
		<superaction name="back" />
	</conflict>

	<conflict name="movement">
		<superaction name="jump" />
		<superaction name="sprint" />
		<superaction name="toggle_crouch" />
		<superaction name="toggle_run" />
		<superaction name="rotate_left" />
		<superaction name="rotate_right" />
	</conflict>

	<conflict name="combat">
		<superaction name="attack1" />
		<superaction name="attack2" />
		<superaction name="attack3" />
		<superaction name="block" />
		<superaction name="weapon1" />
		<superaction name="weapon2" />
		<superaction name="opponent_next" />
		<superaction name="opponent_prev" />
		<superaction name="unlock" />
		<superaction name="surrender" />
		<superaction name="opponent_lock" />
	</conflict>

	<conflict name="inventory_shortcuts">
		<superaction name="inventory" />
		<superaction name="player" />
		<superaction name="horse" />
		<superaction name="quest_log" />
		<superaction name="map" />
		<superaction name="skiptime" />
		<superaction name="codex" />
	</conflict>

	<conflict name="ui_actions">
		<superaction name="move_item" />
		<superaction name="toggle_info" />
		<superaction name="cycle_tabs" />
	</conflict>

	<!-- ============== interactive ground ============== -->
	<!-- general -->
	<conflict>
		<include conflict="directions"/>
		<include conflict="actions"/>
		<include conflict="movement"/>
		<include conflict="combat"/>
		<include conflict="inventory_shortcuts"/>
		<superaction name="screenshot" />
		<superaction name="tobii_enable" />
		<superaction name="companion_interaction" />
	</conflict>

	<!-- attack abort -->
	<conflict>
		<superaction name="attack_abort" />

		<include conflict="directions"/>
		<include conflict="inventory_shortcuts"/>
		<superaction name="back" />
		<superaction name="attack1" />
		<superaction name="weapon1" />
		<superaction name="weapon2" />
		<superaction name="opponent_next" />
		<superaction name="opponent_prev" />
		<superaction name="unlock" />
		<superaction name="screenshot" />
		<superaction name="tobii_enable" />
	</conflict>

	<!-- ============== inventory / menu ============== -->
	<!-- general -->
	<conflict>
		<include conflict="directions"/>
		<include conflict="actions"/>
		<include conflict="ui_actions"/>
		<include conflict="inventory_shortcuts"/>
		<superaction name="screenshot" />
		<superaction name="tobii_enable" />
		<superaction name="ui_map_center" />
	</conflict>

	<!-- ui_clear -->
	<conflict>
		<superaction name="ui_clear" />

		<include conflict="directions"/>
		<include conflict="actions"/>
		<superaction name="screenshot" />
		<superaction name="tobii_enable" />
	</conflict>

	<!-- ============== dialog: skip ============== -->
	<!-- back aborts cutscene, skip_dialog skips part of cutscene ; back skips dialog anyway -->
	<conflict>
		<superaction name="skip_dialog" />
		<superaction name="back" />
		<superaction name="screenshot" />
		<superaction name="tobii_enable" />
	</conflict>

	<!-- ============== dialog: confirm ============== -->
	<!-- directions and primary_action can not be on wheel up or wheel down -->
	<conflict>
		<include conflict="directions"/>
		<superaction name="primary_action" />

		<superaction name="ui_and_dialog_wheel_down" />
		<superaction name="ui_and_dialog_wheel_up" />
	</conflict>

	<!-- directions can not be on left mouse button -->
	<conflict>
		<include conflict="directions"/>
		<superaction name="confirm_dialog_extra"/>
	</conflict>

	<!-- ============== minigames ============== -->
	<!-- alchemy and others -->
	<conflict>
		<superaction name="minigame_action_2" />
		<superaction name="minigame_action_3" />
