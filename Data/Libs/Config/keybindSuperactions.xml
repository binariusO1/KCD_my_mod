<keybinds>
<!--=============== ui groups =================================================-->
	<ui_group name="general" ui_label="@ui_keybinds_group_general" />
	<ui_group name="movement" ui_label="@ui_keybinds_group_movement"/>
	<ui_group name="combat" ui_label="@ui_keybinds_group_combat" />
	<ui_group name="gui" ui_label="@ui_keybinds_group_inventory" />
	<ui_group name="other" ui_label="@ui_keybinds_group_other"/>

<!--============= superactions ================================================-->
