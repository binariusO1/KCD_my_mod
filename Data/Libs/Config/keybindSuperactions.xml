		<action name="helpscreen_left" map="helpscreen" />
		<action name="moveleft" map="player" />
		<action name="sword_position_left" map="sharpening" />
		<action name="read_prev" map="reading" />
		<action name="dice_left" map="dice" />
		<action name="horse_turnleft" map="horse" />
		<action name="lock_dir_bck" map="lockpicking" />
		<action name="turn_left" map="pickpocketing_ui" />

		<control input="a" controller="keyboard" />
		<control input="left" controller="keyboard" />
	</superaction>

	<superaction name="right" ui_group="general" ui_name="@ui_keybinds_right" ui_tooltip="" keyboard="writeable">
